{
  "name": "next-starter-template",
  "description": "Build a full-stack web application with Next.js.",
  "private": true,
  "cloudflare": {
    "label": "Next.js Framework Starter",
    "products": [
      "Workers"
    ],
    "categories": [],
    "preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/05eee9e0-2c04-48fd-11bf-84e65f839000/public",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/4e913a56-fd99-4372-0586-9f1364173100/preview",
    "publish": true
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@opennextjs/cloudflare": "1.14.0",
    "next": "16.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@tailwindcss/postcss": "4.1.4",
    "@types/node": "24.0.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "9.27.0",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "tailwindcss": "4.1.1",
    "typescript": "5.8.3",
    "wrangler": "4.49.1"
  },
  "scripts": {
    "build": "next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "check": "npm run build && tsc",
    "deploy:staging": "opennextjs-cloudflare build && wrangler deploy --env=staging",
    "deploy:production": "opennextjs-cloudflare build && wrangler deploy --env=production",
    "dev": "next dev",
    "format": "prettier src --write",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "start": "next start",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
